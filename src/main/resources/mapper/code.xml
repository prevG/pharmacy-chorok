<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pharm.chorok.web.admin.repository.ADCodeRepository">


	<select id="selectCodes"
		parameterType="com.pharm.chorok.domain.table.TbCommCode"
		resultType="com.pharm.chorok.domain.table.TbCommCode">
		<![CDATA[    
		SELECT *
		  FROM TB_COMM_CODE
		 WHERE DITC_CD = #{ditcCd}
		]]>
	</select>

	
</mapper>